{
    "name": "semotus",
    "description": "A subclass of supertype that synchronizes sets of objects.",
    "homepage": "https://github.com/selsamman/semotus",
    "version": "2.3.13",
    "dependencies": {
        "q": "1.x",
        "supertype": "haven-life/supertype#swat/feature/typescript"
    },
    "devDependencies": {
        "@types/bluebird": "*",
        "@types/chai": "^3.4.34",
        "@types/mocha": "^2.2.39",
        "@types/node": "^7.0.65",
        "@types/q": "*",
        "chai": "3.x",
        "eslint": "3.7.x",
        "istanbul": "0.4.5",
        "mocha": "^3.1.2",
        "q": "*",
        "sinon": "1.15.x",
        "ts-node": "^6.1.0",
        "typescript": "^2.9.1",
        "underscore": "^1.9.1"
    },
    "scripts": {
        "lint": "eslint .",
        "test": "istanbul cover _mocha -- test/*.js -R spec",
        "tsTests": "istanbul cover _mocha -- test/supertype/semotus.js -R spec",
        "debugTSTests": "mocha --inspect-brk test/supertype/semotus.js",
        "debugJSTests": "mocha --inspect-brk test/*.js",
        "test:ts": "npm run compile && npm run tsTests",
        "compile": "node ./node_modules/typescript/bin/tsc"
