    "directories": {},
    "engines": {
        "node": ">=0.8.0"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/selsamman/semotus.git"
    },
    "bugs": {
        "url": "http://github.com/selsamman/semotus/issues"
    },
    "optionalDependencies": {}
